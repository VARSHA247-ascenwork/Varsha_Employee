
@using VarshaWeb.Models;

@{
    ViewBag.Title = "EmployeeDetails";
}

<!-- breadcrumb -->
    <form id="AddEmployeeDetailForm" ng-app="EmployeeApp" ng-controller="EmployeeController">
        <div class="breadcrumb-header justify-content-between">
            <div class="my-auto">
                <div class="d-flex">
                    <h4 class="content-title mb-0 my-auto">Employee Mangment</h4><span class="text-muted mt-1 tx-13 ml-2 mb-0">/ Employee Details</span>
                </div>
            </div>
        </div>
        <!-- breadcrumb -->
        <!-- row -->
        <div class="row">
            <div class="col-xl-12 col-lg-12">


                <!--Entry Code start-->
                <div class="card custom-card">
                    <div class="card-body">

                        <div class="card-body pt-0">
                            <div class="mg-b-50">
                                <div class="row row-sm mg-b-20">
                                    <div class="col-lg-3">
                                        <span class="tx-bold">
                                            <label>First Name</label>
                                        </span>
                                    </div>
                                    <div class="col-lg-3">
                                        <label for="">
                                            @ViewBag.name[0].FirstName

                                        </label>
                                    </div>
                                    <div class="col-lg-3">
                                        <span class="tx-bold">
                                            <label>Last Name</label>
                                        </span>
                                    </div>
                                    <div class="col-lg-3">
                                        <label for="">
                                            @ViewBag.name[0].LastName
                                        </label>
                                    </div>
                                </div>
                                <div class="row row-sm mg-b-20">
                                    <div class="col-lg-3">
                                        <span class="tx-bold">
                                            <label>Employee Code</label>
                                        </span>
                                    </div>
                                    <div class="col-lg-3">
                                        <label for="">
                                            @ViewBag.name[0].EmpCode

