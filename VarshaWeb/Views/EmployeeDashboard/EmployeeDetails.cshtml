                                        <span class="tx-bold">
                                            <label>Marial status</label>
                                        </span>
                                    </div>
                                    <div class="col-lg-3">
                                        <label for="">
                                            @ViewBag.name[0].MaritalStatus

                                        </label>
                                    </div>
                                    <div class="col-lg-3">
