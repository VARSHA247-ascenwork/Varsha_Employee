                                    <div class="col-lg-3">
                                        <label for="">
                                            @ViewBag.name[0].Company
                                        </label>
                                    </div>
                                </div>



                                <div class="row row-sm mg-b-20">
                                    <div class="col-lg-3">
                                        <span class="tx-bold">
                                            <label>Division</label>
                                        </span>
                                    </div>
                                    <div class="col-lg-3">
                                        <label for="">
                                            @ViewBag.name[0].Division

