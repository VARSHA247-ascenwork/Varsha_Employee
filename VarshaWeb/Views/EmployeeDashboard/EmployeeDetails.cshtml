                                        <label for="">
                                            @ViewBag.name[0].JoiningDate

                                        </label>
                                    </div>
                                    <div class="col-lg-3">
                                        <span class="tx-bold">
                                            <label> Date of Birth</label>
                                        </span>
                                    </div>
                                    <div class="col-lg-3">
                                        <label for="">
                                            @ViewBag.name[0].DOB
                                        </label>
                                    </div>
                                </div>
                                <div class="row row-sm mg-b-20">
                                    <div class="col-lg-3">
                                        <span class="tx-bold">
                                            <label>Designation</label>
