<!-- breadcrumb -->
<div class="breadcrumb-header justify-content-between">
    <div class="my-auto">
        <div class="d-flex">
            <h4 class="content-title mb-0 my-auto">Employee Management</h4><span class="text-muted mt-1 tx-13 ml-2 mb-0">/ Employee Profile page</span>
        </div>
    </div>
</div>
<!-- breadcrumb -->
<!-- row -->
<div class="row">
    <div class="col-xl-12 col-lg-12">

        <!--Entry Code start-->
        <div class="card custom-card">
            <div class="card-body">

                <div class="card-body">
                    <div class="row">
                        <div class="col-lg-3">
                            <div class="main-profile-overview">
                                <div class="main-img-user profile-user" style="width:200px; height:200px; margin-top:25px;margin-left:25px;">
                                    <img alt="" src="@Model.empProfilePic.Name"><a class="fas fa-camera profile-edit" href="JavaScript:void(0);" style="margin:12px;"></a>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-9">
                            <div class="row bd-t pd-t-10">
                                <div class="col-lg-12">
                                    <h2 class="main-content-title tx-24 mg-b-1 mg-b-lg-1">@Model.emp_BasicInfo.FullName</h2>
                                </div>
                            </div>
                            <div class="row pd-t-10">
                                <div class="col-lg-3">
                                    <i class="fas fa-map-marker-alt"></i>&nbsp;@Model.emp_CompanyMaster.CompanyName | @Model.emp_CompanyMaster.City, @Model.emp_CompanyMaster.Country
                                </div>
                                <div class="col-lg-3">
                                    <i class="fa fa-envelope-square" aria-hidden="true"></i>&nbsp;<a href="#">@Model.emp_BasicInfo.OfficeEmail</a>
                                </div>
                                <div class="col-lg-3">
                                    <i class="fa fa-phone" aria-hidden="true"></i>&nbsp;@Model.emp_BasicInfo.ContactNumber
                                </div>
                                <div class="col-lg-3">
                                    <i class="fa fa-briefcase" aria-hidden="true"></i>&nbsp;@Model.emp_BasicInfo.Designation
                                </div>
                            </div><br />
                            <div class="row bd-t pd-t-30">
                                <div class="col-lg-3">
                                    <b>Employee Code</b><br />@Model.emp_BasicInfo.EmpCode
                                </div>
                                <div class="col-lg-3">
                                    <b>Department</b><br />@Model.emp_BasicInfo.Department
                                </div>
                                <div class="col-lg-3">
                                    <b>Manager</b><br />@Model.emp_BasicInfo.Manager
                                </div>
                                <div class="col-lg-3">
                                    <b>Marital Status</b><br />@Model.emp_BasicInfo.MaritalStatus
                                </div>
                            </div>
                            <br />
                            <div class="row bd-t pd-t-30">

                                <div class="col-lg-3">
                                    <b>Date of Birth</b><br />@Model.emp_BasicInfo.DOB
                                </div>
                                <div class="col-lg-3">
                                    <b>Date of Joining</b><br />@Model.emp_BasicInfo.JoiningDate
                                </div>
                                <div class="col-lg-3">
                                    <b>Employee Status</b><br />@Model.emp_BasicInfo.EmpStatus
                                </div>
                                <div class="col-lg-3">
                                    <b>Probation Status</b><br />@Model.emp_BasicInfo.ProbationStatus
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--Entry Code End-->
    </div>
</div>
<!-- row closed -->
