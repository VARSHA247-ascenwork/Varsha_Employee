
@model IEnumerable<WizrrWeb.Models.EmployeeManagement.Emp_EducationalDetailsModel>
 
    <div class="col-xl-12">
        <div class="table-responsive">
            <table class="tables" style="width:100%">
                <thead>
                    <tr>
                        <th scope="col">Institute</th>
                        <th scope="col">Degree</th>
                        <th scope="col">Percentage</th>
                        <th scope="col">From</th>
                        <th scope="col">To</th>
                        <th scope="col">Action</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model.Count() > 0)
                    {
                        foreach (var emp in Model)
                        {
                            <tr>
                                <td scope="row" data-label="Institute">@emp.UniversityInstitute</td>
                                <td data-label="Degree">@emp.Degree</td>
                                <td data-label="Percentage">@emp.MarksInPercentage</td>
                                <td data-label="From">@emp.From</td>
                                <td data-label="To">@emp.To</td>
                                <td data-label="Action">
                                    <div class="btn-icon-list center">
                                        <button class="btn" style="padding:0px 5px 0px 30px;margin:0px;" onclick="EditEducation(@emp.ID)"><i class="si si-pencil text-primary mr-2" data-toggle="tooltip" title="" data-placement="top" data-original-title="Edit"></i></button>
                                        <button class="btn" style="padding:0px;margin:0px;" onclick="DeleteEducationDetails(@emp.ID)"><i class="si si-trash text-danger mr-2" data-toggle="tooltip" title="" data-placement="top" data-original-title="Delete"></i></button>
                                    </div>
                                </td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>