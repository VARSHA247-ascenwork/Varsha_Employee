@model WizrrWeb.Models.EmployeeManagement.Emp_PublicationDetailsModel


<form role="form">
    <div class="col-lg-12" style="margin-right:60px;">
        <div class="form-group">
            <label for="Publication_on">Publication On</label>
            <div class="input-group">
                <div class="input-group-prepend">
                    <div class="input-group-text">
                        <i class="typcn typcn-calendar-outline tx-24 lh--9 op-6"></i>
                    </div>
                </div>
                <input class="form-control" id="txt_Publication_date" type="text" value="@Model.PublishedOn" placeholder="DD/MM/YYYY">
            </div>
        </div><div class="form-group">
            <label for="Publication_on">Publication</label>
            <input type="text" value="@Model.Publication" id="txtPublication_on" class="form-control">
        </div>
        <div class="form-group">
            <label for="Description">Description</label>
            <textarea type="text" placeholder="Description" id="Description" class="form-control" row="2">@Model.Description</textarea>
        </div><div class="form-group">
            <label for="url">URL</label>
            <input type="text" id="url" value="@Model.Url" class="form-control">
        </div>
    </div>
</form>