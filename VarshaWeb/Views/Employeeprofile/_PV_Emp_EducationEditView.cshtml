@model WizrrWeb.Models.EmployeeManagement.Emp_EducationalDetailsModel

@using (Html.BeginForm())
{
    <div class="col-lg-12" style="margin-right:60px;">
        <div class="form-group">
            <label for="Institute">Institute/University</label>
            @Html.EditorFor(model => model.UniversityInstitute, new { htmlAttributes = new { @class = "form-control", @id = "Institute" } })
        </div>
        <div class="form-group">
            <label for="Degree">Degree</label>
            @Html.EditorFor(model => model.Degree, new { htmlAttributes = new { @class = "form-control", @id = "Degree" } })
        </div>
        <div class="form-group">
            <label for="Percentage">Percentage/CGPA</label>
            @Html.EditorFor(model => model.MarksInPercentage, new { htmlAttributes = new { @class = "form-control", @id = "Percentage" } })
        </div>
        <div class="form-group" style="left: 1090px;z-index: 100002;display: block;top: 659px;">
            <label for="Education_From_Date">From Date</label>
            <div class="input-group">
                <div class="input-group-prepend">
                    <div class="input-group-text">
                        <i class="typcn typcn-calendar-outline tx-24 lh--9 op-6"></i>
                    </div>
                </div>
                @Html.EditorFor(model => model.From, new { htmlAttributes = new { @class = "form-control", id = "txtEducatinalfromdate" } })
            </div>
        </div><div class="form-group">
            <label for="Education_To_Date">To Date</label>
            <div class="input-group">
                <div class="input-group-prepend">
                    <div class="input-group-text">
                        <i class="typcn typcn-calendar-outline tx-24 lh--9 op-6"></i>
                    </div>
                </div>
                @Html.EditorFor(model => model.To, new { htmlAttributes = new { @class = "form-control", @id = "txtEducatinalTodate" } })
            </div>
        </div>
        <div class="form-group">
            <label for="Subject">Subject</label>
            @Html.EditorFor(model => model.Subject, new { htmlAttributes = new { @class = "form-control", id = "txtSubject" } })
        </div>
    </div>

}