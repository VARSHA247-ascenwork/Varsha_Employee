                                                                    <option value="">
                                                                        Choose one
                                                                    </option>
                                                                    <option value="Married">
                                                                        Married
                                                                    </option>
                                                                    <option value="UnMarried">
                                                                        UnMarried
                                                                    </option>

                                                                </select>
                                                            </div>
                                                            <div id="slErrorContainermarrial"></div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row row-sm">
                                                <div class="col-lg-6">
                                                    <label for="exampleInputPassword1">OnProbationTill</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <div class="input-group-text">
                                                                <i class="typcn typcn-calendar-outline tx-24 lh--9 op-6"></i>
                                                            </div>
                                                        </div><input class="form-control" id="datetimepicker2" ng-model="ngtxtProbationtill" type="text">
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">Probation Status</label>
                                                        <div id="slWrapperProbation" class="parsley-select">
                                                            <select required="" class="form-control select2" ng-model="ngddlProbationstatus" data-parsley-class-handler="#slWrapperProbation" data-parsley-errors-container="#slErrorContainerProbation">
                                                                <option value="">
                                                                    Choose one
                                                                </option>
                                                                <option value="Probation">
                                                                    Probation
                                                                </option>
                                                                <option value="Confirmed">
                                                                    Confirmed
                                                                </option>

                                                            </select>
                                                        </div>
                                                        <div id="slErrorContainerProbation"></div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row row-sm">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">Manager</label>
                                                        <div id="slWrapperManager" class="parsley-select">
                                                            <select required="" class="form-control select2" name="EmployeeName" ng-model="ngddlManager" data-parsley-class-handler="#slWrapperManager" data-parsley-errors-container="#slErrorContainermanager">
                                                                <option value="">

                                                                    Choose one
                                                                </option>
                                                                @foreach (var obj in ViewBag.emp)
                                                                {
                                                                    <option value="@obj.UserNameId">
                                                                        @obj.User_Name
                                                                    </option>
                                                                }
                                                            </select>

                                                        </div>
                                                        <div id="slErrorContainermanager"></div>

                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="Middle_Name">Office Mail</label>
                                                        <input required="" type="email" class="form-control" ng-model="ngtxtOfficemail" id="txtofficemail" placeholder="Enter Office_Mail">
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="row row-sm">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">Mobile Number</label>
                                                        <div class="input-group">
                                                            <input class="form-control" onkeypress="return restrictCharacters(event);" maxlength="10" ng-model="ngtxtmobile" id="phone" name="phone" type="tel">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="Telephone_Number">Telephone Number</label>
                                                        <!--   <input type="tel" name="country_code" id="Telephone_Number" required="" onkeypress="phoneno()" maxlength="10" class="form-control" ng-model="ngtxtmobileno"  placeholder="Enter Telephone Number">-->
                                                        <input type="text" maxlength="10" onkeypress="return restrictCharacters(event);" id="Telephone_Number" class="form-control" ng-model="ngtxtmobileno" placeholder="Enter Telephone Number" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row row-sm">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">Company</label>
                                                        <div id="slWrapperComapany" class="parsley-select">
                                                            <select required="" class="form-control select2" ng-model="ngddlCompany" data-parsley-class-handler="#slWrapperComapany" data-parsley-errors-container="#slErrorContainerCompany">
                                                                <option value="">
                                                                    Choose one
                                                                </option>
                                                                @foreach (VarshaWeb.Models.EmployeeManagement.Emp_CompanyMasterModel company in (List<VarshaWeb.Models.EmployeeManagement.Emp_CompanyMasterModel>)ViewBag.company)
                                                                {
                                                                    <option value="@company.ID">
                                                                        @company.CompanyName
                                                                    </option>
                                                                }
                                                            </select>
                                                        </div>
                                                        <div id="slErrorContainerCompany"></div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">Designation</label>
                                                        <div id="slWrapperDesignation" class="parsley-select">
                                                            <select required="" class="form-control select2" ng-model="ngddlDesignation" data-parsley-class-handler="#slWrapperDesignation" data-parsley-errors-container="#slErrorContainerDesignation">
                                                                <option value="">
                                                                    Choose one
                                                                </option>
                                                                @foreach (VarshaWeb.Models.EmployeeManagement.Emp_DesignationModel designation in (List<VarshaWeb.Models.EmployeeManagement.Emp_DesignationModel>)ViewBag.designation)
                                                                {
                                                                    <option value="@designation.ID">
                                                                        @designation.Designation
                                                                    </option>
                                                                }
                                                            </select>
                                                        </div>
                                                        <div id="slErrorContainerDesignation"></div>
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="row row-sm">
                                                <div class="col-lg-6">
                                                    <div class="form-group">

                                                        <label for="exampleInputPassword1">Department</label>
                                                        <div id="slWrapperDepartment" class="parsley-select">
                                                            <select required="" class="form-control select2" ng-model="ngddlDepartment" data-parsley-class-handler="#slWrapperDepartment" data-parsley-errors-container="#slErrorContainerDepartment">
                                                                <option value="">
                                                                    Choose one
                                                                </option>
                                                                @foreach (VarshaWeb.Models.EmployeeManagement.Emp_DepartmentModel department in (List<VarshaWeb.Models.EmployeeManagement.Emp_DepartmentModel>)ViewBag.department)
                                                                {
                                                                    <option value="@department.ID">
                                                                        @department.DepartmentName
                                                                    </option>
                                                                }
                                                            </select>
                                                        </div>
                                                        <div id="slErrorContainerDepartment"></div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">Division</label>
                                                        <div id="slWrapperDivision" class="parsley-select">
                                                            <select required="" class="form-control select2" ng-model="ngddlDivision" data-parsley-class-handler="#slWrapperDivision" data-parsley-errors-container="#slErrorContainerDivision">
                                                                <option value="">
                                                                    Choose one
                                                                </option>
                                                                @foreach (VarshaWeb.Models.EmployeeManagement.Emp_DivisionModel division in (List<VarshaWeb.Models.EmployeeManagement.Emp_DivisionModel>)ViewBag.division)
                                                                {
                                                                    <option value="@division.ID">
                                                                        @division.Division
                                                                    </option>
                                                                }
                                                            </select>
                                                        </div>
                                                        <div id="slErrorContainerDivision"></div>
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="row row-sm">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">Region</label>
                                                        <div id="slWrapperRegion" class="parsley-select">
                                                            <select required="" class="form-control select2" ng-model="ngddlRegion" data-parsley-class-handler="#slWrapperRegion" data-parsley-errors-container="#slErrorContainerRegion">
                                                                <option value="">
                                                                    Choose one
                                                                </option>
                                                                @foreach (VarshaWeb.Models.EmployeeManagement.Emp_RegionModel Region in (List<VarshaWeb.Models.EmployeeManagement.Emp_RegionModel>)ViewBag.Region)
                                                                {
                                                                    <option value="@Region.ID">
                                                                        @Region.Region
                                                                    </option>
                                                                }
                                                            </select>
                                                        </div>
                                                        <div id="slErrorContainerRegion"></div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="exampleInputPassword1">Branch</label>
                                                        <div id="slWrapperBranch" class="parsley-select">
                                                            <select required="" class="form-control select2" ng-model="ngddlBranch" data-parsley-class-handler="#slWrapperBranch" data-parsley-errors-container="#slErrorContainerBranch">
                                                                <option value="">
                                                                    Choose one
                                                                </option>
                                                                @foreach (VarshaWeb.Models.EmployeeManagement.Emp_BranchModel Branch in (List<VarshaWeb.Models.EmployeeManagement.Emp_BranchModel>)ViewBag.Branch)
                                                                {
                                                                    <option value="@Branch.ID">
                                                                        @Branch.Branch
                                                                    </option>
                                                                }
                                                            </select>
                                                        </div>
                                                        <div id="slErrorContainerBranch"></div>
                                                    </div>

                                                </div>
                                            </div>

                                            <div class="row row-sm">

                                            </div>
                                        </div>
                                        <div class="row row-sm text-center justify-content-center">
                                            <div class="col-sm-6 col-md-3 mg-t-10">
                                                <button type="submit" class="btn btn-primary btn-block">Submit</button>
                                            </div>
                                            <div class="col-sm-6 col-md-3 mg-t-10">
                                                <button type="button" class="btn btn-secondary btn-block" ng-click="CancelAddpage()">Cancel</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal" id="modaldemo4" data-backdrop="static" data-keyboard="false">
                                    <div class="modal-dialog modal-dialog-centered" role="document">
                                        <div class="modal-content tx-size-sm">
                                            <div class="modal-body tx-center pd-y-20 pd-x-20">
                                                <i class="icon ion-ios-checkmark-circle-outline tx-100 tx-success lh-1 mg-t-20 d-inline-block"></i>
                                                <h4 class="tx-success tx-semibold mg-b-20">Congratulations!</h4>
                                                <p class="mg-b-20 mg-x-20">Your request have been successfully submitted.</p><button aria-label="Close" class="btn ripple btn-success pd-x-25" data-dismiss="modal" type="button" ng-click="PerformAction()">Continue</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="modal-footer justify-content-center">
                        <!--<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                        <button type="button" class="btn btn-primary">Save changes</button>  -->
                    </div>
                </div>
            </div>

        </div>

    </div>


    <!-----view  page  Modal --> ModalPopupEdit
    <div class="modal left fade" id="ModalPopupView" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-keyboard="false" data-backdrop="static" aria-hidden="true">

        <!-- Add class .modal-full-height and then add class .modal-right (or other classes from list above) to set a position to the modal -->
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title w-100" id="myModalLabel"></h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body" id="EditModal">
                    <div class="row">
                        <div class="col-xl-12 col-lg-12">


                            <!--Entry Code start-->
                            <div class="card custom-card">
                                <div class="card-body">

                                    <div class="card-body pt-0">
                                        <div class="mg-b-50" ng-repeat="emp in EmpInfo">
                                            <div class="row col-lg-12">
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="Name">First Name</label><p><b>{{emp.FirstName}}</b></p>
                                                    </div><div class="form-group"><label for="Name">Employee Code</label><p><b>{{emp.EmpCode}}</b></p></div>
                                                    <div class="form-group"><label for="Empcode">Joining Date</label><p><b>{{emp.JoiningDate}}</b></p></div>
                                                    <div class="form-group"><label for="DOB">Date Of Birth</label><p><b>{{emp.DOB}}</b></p></div>
                                                    <div class="form-group"><label for="ProbationT">Designation</label><p><b>{{emp.Designation}}</b></p></div>
                                                    <div class="form-group"><label for="Manager">Probation Status</label><p><b>{{emp.ProbationStatus}}</b></p></div>
                                                    <div class="form-group"><label for="Mobile">Manager</label><p><b>{{emp.Manager}}</b></p></div>

                                                    <div class="form-group"><label for="Department">Contact Number </label><p><b>{{emp.ContactNumber}}</b></p></div>
                                                    <div class="form-group"><label for="Department">Region </label><p><b>{{emp.Region}}</b></p></div>
                                                    <div class="form-group"><label for="Department">Office Email </label><p><b>{{emp.OfficeEmail}}</b></p></div>

                                                </div><div class="col-lg-6">
                                                    <div class="form-group">
                                                        <label for="Name">Last Name</label><p><b>{{emp.LastName}}</b></p>
                                                        <div class="form-group"><label for="Department">User Name </label><p><b>{{emp.User_Name}}</b></p></div>
                                                    </div><div class="form-group"><label for="Code">Gender</label><p><b>{{emp.Gender}}</b></p></div>
                                                    <div class="form-group"><label for="Gender">On ProbationTill</label><p><b>{{emp.OnProbationTill}}</b></p></div>
                                                    <div class="form-group"><label for="PStatus">Department</label><p><b>{{emp.Department}}</b></p></div>
                                                    <div class="form-group"><label for="Office">Marital Status</label><p><b>{{emp.MaritalStatus}}</b></p></div>
